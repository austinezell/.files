# testStringBananaCreme

source $HOME/.bash_aliases

[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*

export NVM_DIR="/home/austin/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh"  # This loads nvm
# add gopath

export GOPATH=$HOME/gowork
export PATH=$GOPATH:$PATH:/usr/local/

#adds things that should be in PATH

export PATH=$PATH:$HOME/bin
export PATH=/etc:$PATH/usr/local/sbin:/usr/local/bin:/usr/local:/usr/sbin:/usr/bin:/sbin:/bin:/usr/lib32:/lib

export PS1="\W \$(__git_ps1)$:"

if [ -f /etc/bash_completion ]; then
 . /etc/bash_completion
fi
